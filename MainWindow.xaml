<Window x:Class="SimpleBlobUtility.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SimpleBlobUtility"
        mc:Ignorable="d"
        Background="#0B7A75"
        Title="Azure File Saver" Height="227" Width="420">
    <Grid>
        <Button x:Name="btnSaveFile" Content="Upload File" HorizontalAlignment="Left" Margin="195,36,0,0" VerticalAlignment="Top" Height="54" Width="156" Click="btnSaveFile_Click" FontSize="16" Background="#7B2D26" Foreground="White"/>
        <Button x:Name="btnViewFiles" Content="Azure Files" HorizontalAlignment="Left" Margin="33,36,0,0" VerticalAlignment="Top" Height="54" Width="137" Click="btnViewFiles_Click" FontSize="16" Background="#19535F" Foreground="wHITE">
        </Button>
        <Button Content="Settings" HorizontalAlignment="Left" Margin="33,117,0,0" VerticalAlignment="Top" Height="55" Width="137" Click="Button_Click" FontSize="16" Background="#D7C9AA"/>

    </Grid>
</Window>
